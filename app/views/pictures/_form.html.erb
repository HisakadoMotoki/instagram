<%= form_for(@picture, html: {class: "field", multipart: true}) do |f| %>
  <div class="">
    <div class="form-group">
      <%= f.text_field :title, placeholder: "Title" %>
    </div>
    <div class="form-group">
      <%= f.text_field :description, placeholder: "説明" %>
    </div>
    <%= f.submit "送信" %>
  </div>
<% end %>